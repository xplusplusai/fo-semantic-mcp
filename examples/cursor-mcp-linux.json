{
  "mcpServers": {
    "fo-semantic-mcp": {
      "command": "node",
      "args": ["/path/to/fo-semantic-mcp/dist/server.js"],
      "env": {
        "FOINDEX_API_KEY": "your_api_key_here",
        "FO_SEARCH_DEFAULT_THRESHOLD": "0.5",
        "FO_LOCAL_ASSETS_PATH": "/path/to/your/PackagesLocalDirectory"
      }
    }
  }
}